{
  "name": "formit-forma-login-wrapper",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev-proxy": "npm run build && SET NODE_ENV=development && SET DEV_SERVER=1 && vite",
    "build": "npm run clean_build && npm run create_version && npm run create_prerelease",
    "clean_build": "tsc && npm run delete && vite build",
    "delete": "rimraf build && rimraf docs/dev",
    "create_dist": "npm run prepare_version && cpx versions_prerelease.json docs && npm run prepare_dist && npm run copy_assets",
    "prepare_dist": "cpx versions.json docs && cpx manifest.json %npm_config_directory% && cpx plugin_formit.js %npm_config_directory%",
    "prepare_version": "cpx 'build/*' '%npm_config_directory%' --verbose && cpx README.md docs",
    "copy_assets": "cpx 'src/assets/*' '%npm_config_directory%/assets' --verbose && cpx 'build/assets/*' '%npm_config_directory%/assets' --verbose && cpx 'src/Addins/*' '%npm_config_directory%' --verbose",
    "replace_prerelease": "cpx 'src/Addins/PreRelease/*' '%npm_config_directory%' --verbose",
    "create_version": "npm run create_dist --directory=docs/v24_1_0",
    "create_prerelease": "npm run create_dist --directory=docs/dev",
    "lint": "prettier --write . && prettier --check .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formit3d/FormItFormaLoginWrapper.git"
  },
  "author": "Autodesk",
  "dependencies": {
    "cpx": "^1.0",
    "eslint": "^8.16.0",
    "husky": "^8.0.0",
    "prettier": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "rimraf": "^5.0.0",
    "vite": "^4.1.0",
    "mkdirp": "^3.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  }
}
